# Horizon Routing

Options -Indexes

<IfModule mod_rewrite.c>
	RewriteRule ^composer - [R=404]
	RewriteRule ^vendor(/(.*))?$ - [R=404]
	RewriteRule ^translations(/(.*))?$ - [R=404]
	RewriteRule ^views(/(.*))?$ - [R=404]
	RewriteRule ^private(/(.*))?$ - [R=404]
	RewriteRule ^src(/(.*))?$ - [R=404]
	RewriteRule ^routes(/(.*))?$ - [R=404]
	RewriteRule ^config(/(.*))?$ - [R=404]
	RewriteRule ^error_log$ - [R=404]
</IfModule>
